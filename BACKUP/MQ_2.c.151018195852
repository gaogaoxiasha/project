#include "MH_B.h"

/*--------------------------  D E F I N I T I O N  ---------------------------*/

/*---------------------------  F U N C T I O N S  ----------------------------*/
void MH_B_Init(void)
{
 	 DDRA &= ~(0x01<<6);
}  

unsigned char Read_MH_B(void)  
{
    unsigned char state = 0;
	state = PINA&(0x01<<6);
	return state;
}


